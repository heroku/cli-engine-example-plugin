{
  "name": "cli-engine-example-plugin",
  "version": "2.1.2",
  "author": "Jeff Dickey @dickeyxxxx",
  "dependencies": {
    "cli-engine-command": "3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "flow-bin": "0.43.1",
    "flow-copy-source": "1.1.0",
    "flow-typed": "2.0.0",
    "jest": "19.0.2",
    "rimraf": "2.6.1",
    "standard": "10.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src"
  },
  "license": "ISC",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib --ignore '*.test.js'",
    "clean": "rimraf lib",
    "copy-flow": "flow-copy-source -v -i '**/*.test.js' src lib",
    "prepare": "npm run clean && npm run build && npm run copy-flow",
    "release": "np",
    "test": "jest && flow && standard && npm run prepare"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": "jest",
    "globals": [
      "Class",
      "$Shape"
    ],
    "ignore": [
      "lib",
      "flow-typed"
    ]
  }
}
